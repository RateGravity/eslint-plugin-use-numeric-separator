{
  "name": "eslint-plugin-use-numeric-separator",
  "version": "2.0.0",
  "description": "An ESLint plugin to enforce usage of the numeric separator for long numeric literals",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/RateGravity/eslint-plugin-use-numeric-separator.git",
  "author": "Own Up <dev+eslint@ownup.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config rollup.config.js && tsc --build tsconfig.build.json",
    "deploy": "node ./scripts/deploy.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint src --ext ts",
    "test": "ts-mocha src/**/__tests__/*.test.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/eslint": "^7.2.6",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "mocha": "^11.1.0",
    "prettier": "^3.1.1",
    "rollup": "^2.34.2",
    "rollup-plugin-babel": "^4.4.0",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "eslint": "^7 || ^8"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "numeric-separator",
    "large-numbers"
  ]
}
