{
  "name": "eslint-plugin-use-numeric-separator",
  "version": "2.0.0",
  "description": "An ESLint plugin to enforce usage of the numeric separator for long numeric literals",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/RateGravity/eslint-plugin-use-numeric-separator.git",
  "author": "Own Up <dev+eslint@ownup.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config rollup.config.js && tsc --build tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint src --ext ts",
    "test": "ts-mocha src/**/__tests__/*.test.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/eslint": "^7.2.6",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "rollup": "^2.34.2",
    "rollup-plugin-babel": "^4.4.0",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "eslint": "^7"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "numeric-separator",
    "large-numbers"
  ]
}
